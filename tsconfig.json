{
    "compilerOptions": {
        "strict": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "lib": ["es6", "dom", "es2016"],
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "exclude": ["node_modules", "build", "webpack", "jest"]
}
